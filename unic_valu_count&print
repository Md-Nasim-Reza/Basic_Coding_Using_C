
#include<stdio.h>
main(){
    int i,x,n,count=0,arr[100010]={0};
    scanf("%d", &n);
    for(i=0; i<n; i++){
        scanf("%d", &x);
        arr[x]++;
    }
    for(i=0; i<=n; i++){
        if(arr[i]){
            printf("%d ", i);
            count++;
        }
    }

    printf("\n%d", count);
}
