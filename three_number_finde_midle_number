
#include<stdio.h>
main(){
int x,y,z;
printf("entr the frist number: ");
    scanf("%d", &x);
printf("enter the number second number: ");
    scanf("%d", &y);
printf("enter the thred number: ");
    scanf("%d", &z);
if((x>y && x<z) || (x>z && x<y))
    printf("%d", x);
else if((y>x && y<z) || (y>z && y<x))
    printf("%d", y);
else
    printf("%d", z);
}
