#include<stdio.h>
#include<string.h>
main(){
char str[50];
int i,l;
puts("please enter the str: ");
gets(str);
l = strlen(str);
printf("char of len: %d\n", l);
for(i=0; i<=l; i++){
    if(str[i] == 'a'|| str[i] == 'e'|| str[i] == 'i'|| str[i] == '0'|| str[i] == 'u')
    continue;
    printf("%c", str[i]);
}
}
