#include<stdio.h>
main(){
    int i,j,n,p=0;
    scanf("%d", &n);
    for(i=2; i<=n-1; i++){
        if(n%i==0)
            p=1;
            break;
    }
    if(p==1)
        printf("not prime\n");
    else
        printf("prime\n");
}


